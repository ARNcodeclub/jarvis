ACTIVE*MODULE*KODI==
VALIDER==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Select","id":1}'
GAUCHE==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Left","id":1}'
DROITE==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Right","id":1}'
HAUT==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Up","id":1}'
BAS==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Down","id":1}'
PAUSE|JOUER|PLAYE==callUrlKodi '{"jsonrpc":"2.0","method":"Player.PlayPause","params":{"playerid":1},"id":1}'
STOP==callUrlKodi '{"jsonrpc":"2.0","method":"Player.Stop","params":{"playerid":1},"id":1}'
MUET|MIOUTE==callUrlKodi '{"jsonrpc":"2.0","method":"Application.SetMute","params":{"mute":true},"id":1}'
NONMUET|NONMIOUTE==callUrlKodi '{"jsonrpc":"2.0","method":"Application.SetMute","params":{"mute":false},"id":1}'
RETOUR|BACK==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Back","id":1}'
*INFO*FILME==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Info","id":1}'
ACTIVER*SOUTITRE==callUrlKodi '{"jsonrpc":"2.0","id":1,"method":"Player.SetSubtitle","params":{"playerid":1,"subtitle":"on"}}'
DESACTIVER*SOUTITRE==callUrlKodi '{"jsonrpc":"2.0","id":1,"method":"Player.SetSubtitle","params":{"playerid":1,"subtitle":"off"}}'
SOUTITRESUIVANT==callUrlKodi '{"jsonrpc":"2.0","id":1,"method":"Player.SetSubtitle","params":{"playerid":1,"subtitle":"next"}}'
PLEINECRAN==callUrlKodi '{"jsonrpc":"2.0","method":"GUI.SetFullscreen","params":{"fullscreen":"toggle"},"id":1}'
MENUPRINCIPAL==callUrlKodi '{"jsonrpc":"2.0","method":"Input.Home","id":1}'
QUIT==callUrlKodi '{"jsonrpc":"2.0","method":"Application.Quit","params":{"playerid":1},"id":1}'
RELANCERKODI==callUrlKodi '{"jsonrpc":"2.0","method":"Application.Quit","id":1}' && sleep 3 && kodi&
LANCERKODI==kodi&
*LISTE*FILMES==callUrlKodi '{"jsonrpc":"2.0","method":"GUI.ActivateWindow","params":{"window":"video"},"id":1}'
*LISTE*MUSIQUES==callUrlKodi '{"jsonrpc":"2.0","method":"GUI.ActivateWindow","params":{"window":"music"},"id":1}'
*LISTE*PHOTOS==callUrlKodi '{"jsonrpc":"2.0","method":"GUI.ActivateWindow","params":{"window":"pictures"},"id":1}'
